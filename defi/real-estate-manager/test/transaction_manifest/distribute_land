TAKE_FROM_WORKTOP_BY_AMOUNT Decimal("1") ResourceAddress("${HOUSE}") Bucket("house2");
CALL_METHOD ComponentAddress("${USER2_ACC}") "deposit" Bucket("land2");
CALL_METHOD ComponentAddress("${USER2_ACC}") "deposit" Bucket("house2");

